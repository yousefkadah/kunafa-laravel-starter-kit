{
    "name": "yousefkadah/kunafa-laravel-starter-kit",
    "description": "A modern Laravel starter kit featuring Kunafa Dashboard with Vue 3, TypeScript, Tailwind CSS, and Inertia.js integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yousef Kadah",
            "email": "example@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "inertiajs/inertia-laravel": "^1.0",
        "tightenco/ziggy": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Kunafa\\StarterKit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kunafa\\StarterKit\\KunafaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
